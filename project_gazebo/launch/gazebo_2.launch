<launch>
    <node name="robot_state_publisher_node" pkg="robot_state_publisher" type="robot_state_publisher">
        <param name="robot_description" value="cat $(find proyecto)/urdf/proyecto.urdf" />
    </node>

    <include file="$(find gazebo_ros)/launch/empty_world.launch">
        <node
            name="spawn_model"
            pkg="gazebo_ros"
            type="spawn_model"
            args="-topic robot_description -entity proyecto" />
    </include>
</launch>

